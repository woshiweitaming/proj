page{
	background: #151a25;
	height: 100%;
}
/**
 * 通用
 */
.main_class{
	padding: 0 20rpx 20rpx 20rpx;
	box-sizing: border-box;
}
.main_class.other{
	padding-top: 0;
}
.main_class.ios,
.main_class.android{
	padding-top: 88rpx;
}
.title_text{
	font-size: 48rpx;
	color: rgba(255,255,255,.9);
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	box-sizing: border-box;
}
.title_text .back_arrow{
	font-size: 50rpx;
	padding-right: 40rpx;
}
.sub_title_text{
	font-size: 28rpx;
	color: rgba(255,255,255,.5);
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-bottom: 60rpx;
}
.red{
	color: #c12d3b !important
}
.green{
	color: #27c173 !important
}
/**
 * 表单类
 */
.form_class .form_label{
	width: 100%;
	margin: 10px 0;
	color: #5a6886;
	font-size: 32rpx;
	padding: 10px;
	box-sizing: border-box;
	border-bottom: 1px solid rgba(255,255,255,.1);
}
.form_class .form_label .form_label_name{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-bottom: 10px;
	box-shadow: rgba(255,255,255,.2);
	line-height: 60rpx;
	display: flex;
	font-size: 28rpx
}
.form_class .form_label .form_input_bar{
	position: relative;
}
.form_class .iconfont{
	font-size: 40rpx;
	padding-right: 10px;
}
.form_class .input_bar{
	font-size: 36rpx;
	color: #eead22;
}
.form_class .button{
	background: #eead22;
	border-radius: 80rpx;
	height: 80rpx;
	color: #151A25;
	text-align: center;
	line-height: 80rpx;
	margin-top: 20px;
	font-size: 30rpx
}
/**
 * 非标题栏标题
 */
.nav_title{
	line-height: 100rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-bottom: 20rpx;
	color: #fff;
	font-weight: bold;
}
.nav_title .icon{
	font-size: 48rpx
}
/**
 * 表格
 */
.table_class{
	position: relative;
	height: 100%;
}
.table_class .table_menu{
	width: 80%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-top: 20rpx;
	border-bottom: 1px solid rgba(255,255,255,.03);
	margin: auto;
}
.table_class .table_menu .menu_label{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	color: rgba(255,255,255,.5);
	padding: 0 10rpx;
	line-height: 100rpx;
	font-size: 30rpx;
	text-align: center;
}
.table_class .table_menu .menu_label.on{
	border-bottom: 2px solid #EE9900;
	color: #EE9900;
}
.table_class .table_hd{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	line-height: 60rpx;
}
.table_class .table_hd_label,
.table_class .table_bd_label{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	font-size: 28rpx;
	line-height: 80rpx;
	color: rgba(255,255,255,.6);
	text-align: center;
	box-sizing: border-box;
}
.table_class .table_bd_label{
	font-size: 26rpx;
	overflow: hidden;
    text-overflow:ellipsis;  
    white-space: nowrap;
}
.table_class .table_hd_label:first-child,
.table_class .table_bd_label:first-child{
	text-align: left;
	padding-left: 20rpx;
}
.table_class .table_hd_label:last-child,
.table_class .table_bd_label:last-child{
	text-align: right;
	padding-right: 20rpx;
}
.table_class .table_bd .table_bd_items{
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	background: #1c222f;
	box-sizing: border-box;
	margin-bottom: 10px;
}
/**
 * 统计
 */
.total_data{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-bottom: 10rpx;
	text-align: center;
	border: 1px solid rgba(255,255,255,.05);
	margin-top: 20rpx;
}
.total_data .data_items{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	background: rgba(255,255,255,.02);
	padding: 10rpx 0;
}
.total_data .data_items .name,
.total_data .data_items .price{
	display: block;
	line-height: 60rpx;
	color: #EE9900;
}
.total_data .data_items .name{
	font-size: 28rpx;
	color: rgba(255,255,255,.5);
}

